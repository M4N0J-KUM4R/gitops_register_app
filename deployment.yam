apiVersion: apps/v1
kind: Deployment
metadata:
  name: register_app_deployment
spec:
  replicas: 2
  selector:
    matchLabels:
      app: register_app
  template:
    metadata:
      labels:
        app: register_app
    spec:
      containers:
        - name: register_app
          image: m4n0jkum4r/register_app_pipeline:1.0.0-18
          resources:
            limits:
              memory: "256Mi"
              cpu: "500m"
          ports:
            - containerPort: 8080
